    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.0.5",
    "remove-files-webpack-plugin": "^1.4.3",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
